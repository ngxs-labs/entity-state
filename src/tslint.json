{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "entity",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "entity",
      "kebab-case"
    ],
    "import-blacklist": [
      true,
      "entity-state",
      "@ngxs-labs/entity-state"
    ]
  }
}
